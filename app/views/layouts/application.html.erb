<!DOCTYPE html>
<html>
<head>
  <title>Klairvoyantsite</title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "http://fonts.googleapis.com/css?family=Tangerine" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="headSection">
  Klairvoyant  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Infosolutions


 <div id="navigation">

   <ul class="dropdown">
     <li><a href="<%= url_for(:action => 'index', :controller => 'welcome') %>">Home</a>
     </li>
     <li><a href="<%= url_for(:action => 'services', :controller => 'welcome') %>">Services</a>
       <ul class="sub_menu">
         <li><a href="<%= url_for(:action => 'rails', :controller => 'welcome') %>" >Ruby On Rails</a></li>
         <li><a href="<%= url_for(:action => 'design', :controller => 'welcome') %>">Web Development</a></li>
         <li><a href="<%= url_for(:action => 'graphic', :controller => 'welcome') %>">Web Designing</a></li>
         <li><a href="<%= url_for(:action => 'marketing', :controller => 'welcome') %>">Marketing</a></li>
       </ul>
     </li>
     <li><a href="<%= url_for(:action => 'contact', :controller => 'welcome') %>">Contact</a>
     </li>
 </ul>
 </div>

</div>





<%= yield %>

<div id="footerSection">

  <div id="addThis">

    <!-- AddThis Button BEGIN -->
    <div class="addthis_toolbox addthis_default_style addthis_32x32_style">
      <a class="addthis_button_preferred_1"></a>
      <a class="addthis_button_preferred_2"></a>
      <a class="addthis_button_preferred_3"></a>
      <a class="addthis_button_preferred_4"></a>
      <a class="addthis_button_compact"></a>
      <a class="addthis_counter addthis_bubble_style"></a>
    </div>
    <script type="text/javascript" src="http://s7.addthis.com/js/250/addthis_widget.js#pubid=klairvoyant"></script>
    <!-- AddThis Button END -->
  </div>

  <div id='translate'>
    <!-- Google Translate Element -->
    <div id="google_translate_element" style="display:block"></div><script>
      function googleTranslateElementInit() {
          new google.translate.TranslateElement({pageLanguage: "en"}, "google_translate_element");
      };</script>
    <script src="http://translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
  </div>
</div>

</body>
</html>
